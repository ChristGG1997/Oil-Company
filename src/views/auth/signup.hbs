<div class="container p-4">
    <div class="row">
        <div class="col-md-4 mx-auto">
            <div class="card text-center">
                <div class="card-header">
                    <h3>Registro</h3>
                    <div class="card-body">
                        <form action="/signup" method="POST">
                        <label for="username" class="pt-4">Nombre de Usuario:</label>
                        <div class="form-group">
                            <input type="text" class="form-control" name="username">
                        </div>
                        <label for="password">Contrase√±a:</label>
                        <div class="form-group">
                            <input type="password" class="form-control" name="password">
                        </div>
                        <label for="cedula">Cedula:</label>
                        <div class="form-group">
                            <input type="number" class="form-control" name="cedula">
                        </div>
                        <label for="names">Nombre:</label>
                        <div class="form-group">
                            <input type="text" class="form-control" name="names">
                        </div>
                        <label for="lastnames">Apellidos:</label>
                        <div class="form-group">
                            <input type="text" class="form-control" name="lastnames">
                        </div>
                        <label for="email">Correo Electronico:</label>
                        <div class="form-group">
                            <input type="email" class="form-control" name="email">
                        </div>
                        <label for="celular">Numero Celular:</label>
                        <div class="form-group">
                            <input type="number" class="form-control" name="celular">
                        </div>
                        <label for="direccion">Direccion:</label>
                        <div class="form-group">
                            <input type="text" class="form-control" name="direccion">
                        </div>
                        <div class="form-group p-4">
                            <button class="btn btn-primary btn-block">
                                Registrar
                            </button>
                        </div>
                        </form>
                        <a href="/auth/profileAdmin"><button class="btn btn-primary btn-block col-md-9 mx-auto">
                                Atras
                            </button></a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>